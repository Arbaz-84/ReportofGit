package stringReverse;

public class StringBuilder2 {
	public static void main(String[] args) {
		String name1="Core";
		String name2="Jave";
		//Useing Concat() method 
		String name3=name1.concat(name2);
		String name4="Basic";
		String name5=name3.concat(name4);
		System.out.println("Name1 value: "+name1);
		System.out.println("Name2 value: "+name2);
		System.out.println("Add Name1&Name2:"+name3);
		System.out.println("Add Name3&name4: "+name5);
	
		;
	}

}
