package AbstractionProgram;
//Abstract class
abstract class Bike
{
abstract void run();	
}
class Honda extends Bike
{
void run()
{
System.out.println("running safely");	
}
}
public class Program2
{
   public static void main(String[] args)  
   {
	//create child class object
	   Honda obj=new Honda();
	   obj.run();
   }
}
